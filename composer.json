{
    "name": "jvalentini/composer-exec",
    "type": "composer-plugin",
    "autoload": {
        "psr-0": {
            "jvalentini\\Composer\\Exec": "src/"
        }
    },
    "extra": {
        "class": "\\jvalentini\\Composer\\Exec\\ExecPlugin"
    },
    "description": "Execute composer package binaries from anywhere in your project.",
    "require": {
        "composer-plugin-api": "~1.0"
    },
    "require-dev" : {
        "phpunit/phpunit" : "~4.4",
        "composer/composer" : "1.*@dev"
    },
